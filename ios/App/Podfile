require_relative '../../node_modules/@capacitor/ios/scripts/pods_helper'

platform :ios, '13.0'
use_frameworks!

def capacitor_pods
  # Automatic Pods
  # Core
  pod 'Capacitor', :path => '../../node_modules/@capacitor/core'
  pod 'CapacitorCordova', :path => '../../node_modules/@capacitor/ios'

end

target 'App' do
  capacitor_pods
  # Add your Pods here
end
