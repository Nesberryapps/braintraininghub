require_relative '../../node_modules/@capacitor/ios/scripts/pods_helper'

platform :ios, '13.0'
use_frameworks!

def capacitor_pods
  # Automatic Pods installer
  #
  # This function will look for all plugins in your `package.json` and install
  # any iOS pods that it finds.
  #
  pod 'Capacitor', :path => '../../node_modules/@capacitor/ios'
  pod 'CapacitorCordova', :path => '../../node_modules/@capacitor/ios'
  
  # Do not add pods here.
  #
  # If you need to add a new pod, you should do so by creating a new plugin
  # and adding the pod to the `.podspec` file.
  #
  # For more information, see https://capacitorjs.com/docs/plugins/ios
  #
  install_pods_from_package_json
end

target 'App' do
  capacitor_pods
end
